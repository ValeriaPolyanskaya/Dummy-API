		{
			"name": "Create user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.environment.set(\"email\",jsonData.email);\r",
							"pm.environment.set(\"userid\",jsonData.id);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"id\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Test for digits in firstName\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var firstName = jsonData.firstName;\r",
							"    var regex = /\\d/;\r",
							"    pm.expect(regex.test(firstName)).to.equal(false);\r",
							"});\r",
							"\r",
							"pm.test(\"Test for symbols in firstName\", function () {\r",
							"var jsonData = pm.response.json();\r",
							"var firstName = jsonData.firstName;\r",
							"var regex = /[!@#$%^&*(),.?\":{}|<>]/;\r",
							"pm.expect(regex.test(firstName)).to.equal(false);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Test for youpmail domain in email\", function () {\r",
							"var jsonData = pm.response.json();\r",
							"var email = jsonData.email;\r",
							"pm.expect(email).to.include(\"@yopmail.com\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Test for phone starting with +375 and containing exactly 7 digits\", function () {\r",
							"var jsonData = pm.response.json();\r",
							"var phone = jsonData.phone;\r",
							"pm.expect(phone).to.match(/^\\+375\\d{7}$/);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
